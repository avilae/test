    <!--
        Naming Convention for Directory Monitoring root:\monitor\COMPANY-CODE
            Examples: (Development) c:\monitor\AMAZT

        File Identifier should use Company Code, matching monitor sub directory
    -->
    <bean id="iCimsAmazon" class="com.abc.polling.PollDirectory">
        <property name="directory" value="file:/@SERVERFILES_MONITOR_PATH@/monitor/AMAZT"/>
        <property name="fileIdentifier" value="AMAZT"/>
        <property name="fileStartsWith" value="Request_AMAZT_"/>
        <property name="fileExtension" value="csv"/>
        <property name="fireOnCreated" value="true"/>
        <property name="fileCreatedActions">
            <list>
                <ref bean="ProcessAmazonBatch"/>
				<ref bean="GenerateAmazonResultFile"/>
                <ref bean="RenameFile"/>
            </list>
        </property>
        <property name="emailRecipients" value="@EMAIL_RECIPIENTS@"/>
    </bean>

