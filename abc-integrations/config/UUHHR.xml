	<!-- Define Data Blocks -->
	<bean id="uuhhr_sequence" class="com.abc.integration.batch.datablock.DataBlockSequence">
		<property name="relaxedDataBlockSequence" value="true" />
		<property name="dataBlockSequence">
			<list>
				<ref bean="uuhhr_header" />
				<ref bean="uuhhr_subject" />
				<ref bean="uuhhr_mvr" />
				<ref bean="uuhhr_emp" />
				<ref bean="uuhhr_edu" />
				<ref bean="uuhhr_ref" />				
				<ref bean="uuhhr_plv" />
				
			</list>
		</property>
	</bean>



	<!-- Batch Context -->
 	<bean id="uuhhrBatchContext" class="com.abc.integration.batch.BatchContext">
    	<property name="contextName" value="UUHHR - CSV"/>
    	<property name="readerType" value="CSV"/>
        <property name="skipRow">
        	<list>
				<value>1</value>
			</list>
		</property>
		<property name="dataBlockSequence">
			<ref bean="uuhhr_sequence" />
		</property>		
		<property name="createResponseBatchFile" value="true" />
		<property name="batchResultContextName" value="UUHHR" />
		<property name="responseContext">
			<bean class="com.abc.integration.batch.response.ResponseContext">
					<property name="processor">
					<bean class="com.abc.integration.batch.response.ResponseContextUUHHR" />
				</property>
				<property name="processorConfig">
					<bean class="com.abc.integration.batch.response.ProcessorConfig">
						<property name="reportUrl"
							value="@NP_DOMAIN@/JSP/ViewReport" />
					</bean>
				</property>
			</bean>
		</property>
        <property name="mapperType" value="SMART"/>
        <property name="checkDeuplicatesInBatch" value="true"/>
        <property name="orderProcessorServiceName" value="DEFAULT"/>
    </bean>

</beans>