<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<!-- 

notificationConfiguration values:
A = ALL
R = Requestor only 
C = Candidate only

Global one can't be included if company has a package that they don't want EDOC setup for.

GLOBAL SETTING SAMPLE 
<bean class="com.abc.service.doccollection.DefaultEDocRule">
    <property name="notificationConfiguration" value="A"/>	
	<property name="beanName" value="com.abc.bo.hbm.Search"/>
	<property name="propName" value="subStatus"/>
	<property name="expectedResult" value="unable to verify"/>
</bean>	 
 -->
 
 <!--  global setting for all packages where notification goes to rec and can -->
 <bean id="DEFAULT_GLOBAL_ALL" class="com.abc.service.doccollection.DefaultEDocRule">
 	<property name="notificationConfiguration" value="A"/>				
	<property name="beanName" value="com.abc.bo.hbm.Search"/>
	<property name="propName" value="subStatus"/>
	<property name="expectedResult" value="unable to verify"/>
 </bean>
  <!--  global setting for all packages where notification goes to CANDIDATE only -->
 <bean id="DEFAULT_GLOBAL_CAN" class="com.abc.service.doccollection.DefaultEDocRule">
 	<property name="notificationConfiguration" value="C"/>				
	<property name="beanName" value="com.abc.bo.hbm.Search"/>
	<property name="propName" value="subStatus"/>
	<property name="expectedResult" value="unable to verify"/>
 </bean>
 
	<bean id="ABTECH" class="com.abc.service.doccollection.EDocConfig">			
		<property name="eDocRules">
			<list>	 			
				<bean class="com.abc.service.doccollection.DefaultEDocRule">
				    <property name="notificationConfiguration" value="A"/>
					<property name="packageCode" value="1973"/>
					<property name="beanName" value="com.abc.bo.hbm.Search"/>
					<property name="propName" value="subStatus"/>
					<property name="expectedResult" value="unable to verify"/>
				</bean>				
				<bean class="com.abc.service.doccollection.DefaultEDocRule">
				    <property name="notificationConfiguration" value="R"/>
					<property name="packageCode" value="1979"/>
					<property name="beanName" value="com.abc.bo.hbm.Search"/>					
					<property name="propName" value="subStatus"/>
					<property name="expectedResult" value="unable to verify"/>
				</bean>				
				<bean class="com.abc.service.doccollection.DefaultEDocRule">
				    <property name="notificationConfiguration" value="C"/>
