	<queryString><![CDATA[SELECT i.requestor_id,i.search_id, i.company_name, 
i.client_ref_number, i.package_req_id, i.search_type_code,
case  when 
i.search_type_code like '%pk'
then (select upper(pkg_name) from package with(nolock) 
	where package.package_Code in ( select pkg_code from search where search_Id = i.search_Id ))
else SEARCH_TYPE.search_type end as search_type, 
i.search_status, 
case when SEARCH_TYPE.search_type_category not in ('civ','cri') then upper(isnull(i.sbj_name,'')) else '' end as sbj_name, 
case when SEARCH_TYPE.search_type_category in ('civ','cri') then upper(isnull(i.sbj_name,''))  else '' end as cri_sbj_name, 
case when SEARCH_TYPE.search_type_category in ('civ','cri') then upper(isnull(i.AKA1,'')) else '' end as AKA1,
case when SEARCH_TYPE.search_type_category in ('civ','cri') then upper(isnull(i.AKA2,'')) else '' end as AKA2,
case when SEARCH_TYPE.search_type_category in ('civ','cri') then upper(isnull(i.AKA3,'')) else '' end as AKA3,
i.dl_number,
i.Edited_County, i.Edited_State, i.Price, i.Statutory_Fee, 
cast(i.Copies as Money) Copies, i.Addl_Year_Fee, i.Return_Date, i.Invoice_Date, 
i.company_code, i.Document_Fee, [user_first_name] +' '+[user_last_name] AS [Requestor Name], 
i.invoice_only, 
case i.sbj_name when ',' 
	then 'xxx-xx-'+substring(sbj_ssn, 6,4)+substring(sbj_ssn, 6,4)+','+sbj_name 
	end as maskSnn,
isnull([client_ref_number],'') + isnull([ref_number2],'') + isnull([ref_number3],'') + isnull([ref_number4],'') + isnull([ref_number5],'') AS sort_group, 
i.ref_number5, i.ref_number4, i.ref_number3, i.ref_number2, 
i.company_code, COMPANY_CONFIG.addl_ref1_lable, COMPANY_CONFIG.addl_ref2_lable, 
COMPANY_CONFIG.addl_ref3_lable, COMPANY_CONFIG.addl_ref4_lable
FROM INVOICE_NEW i
INNER JOIN ABCUSER with(nolock) ON i.requestor_id = ABCUSER.user_id
INNER JOIN SUBJECT with(nolock) ON i.subject_id = SUBJECT.subject_id
INNER JOIN COMPANY_CONFIG with(nolock) ON i.company_code = COMPANY_CONFIG.company_code
INNER JOIN SEARCH_TYPE with(nolock) ON i.search_type_code = SEARCH_TYPE.search_type_code
WHERE i.invoice_only='1' and i.company_code = $P{companyCode}
order by [Requestor Name],[client_ref_number],[package_req_id],i.[search_type_code]]]></queryString>

