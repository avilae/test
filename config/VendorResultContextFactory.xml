  	<bean singleton="false" id="eScreenRequestStatusInterceptor" 
  		class="com.abc.service.integrations.vendor.fw.escreen.interceptors.EscreenRequestStatusInterceptor"/>
	
  	<bean singleton="false" id="eScreenNegativeResultInterceptor" 
  		class="com.abc.service.integrations.vendor.fw.escreen.interceptors.NegativeResultInterceptor"/>
	
  	<bean singleton="false" id="eScreenNonNegativeResultInterceptor" 
  		class="com.abc.service.integrations.vendor.fw.escreen.interceptors.NonNegativeResultInterceptor"/>
	
  	<bean singleton="false" id="eScreenCommitInterceptor" 
  		class="com.abc.service.integrations.vendor.fw.escreen.interceptors.CommitInterceptor"/>
  	
  	<bean singleton="false" id="eScreenNotificationInterceptor" 
  		class="com.abc.service.integrations.vendor.fw.escreen.interceptors.NotificationInterceptor"/>
	
  	<bean singleton="false" id="eScreenResponseInterceptor" 
  		class="com.abc.service.integrations.vendor.fw.escreen.interceptors.ResponseInterceptor"/>
	
  	<bean id="eScreenResultProcessContext" class="com.abc.service.integrations.vendor.fw.VendorResultProcessContext" >
  		<property name="contextName" value="ESCREEN"/>
  		<property name="requestProcessor" ref="eScreenRequestProcessor"/>
  	</bean>
  	
  	<bean id="eScreenNegativeDiluteInterceptor" 
  		  class="com.abc.service.integrations.vendor.fw.escreen.interceptors.NegativeDiluteResultInterceptor"
  		  singleton="false" >
  		  	<!-- special handing client list -->
	  		<property name="clientCodes" >
		        <list>
		           <!-- <value>eScreen client codes</value>  -->		  			      
		        </list>
	  		</property>	  			
	  		<property name="desiredResultStatus" value="Dilute"/>
  	</bean>
  		 
  	<bean id="eScreenRequestProcessor"  class="org.springframework.aop.framework.ProxyFactoryBean">
		<property name="target">
			<bean class="com.abc.service.integrations.vendor.fw.escreen.EscreenRequestProcessor"/>
		</property>
		<property name="proxyTargetClass"><value>true</value></property>
		<property name="interceptorNames">
		<list>
			<!-- Interceptors -->
			<value>eScreenDuplicateResultCheckInterceptor</value>
			<value>eScreenFinderInterceptor</value>			
			<value>eScreenResultStatusInterceptor</value>
			<value>eScreenRequestStatusInterceptor</value>	
			<value>eScreenNegativeDiluteInterceptor</value>		
			<value>eScreenNegativeResultInterceptor</value>
			<value>eScreenNonNegativeResultInterceptor</value>
			<value>eScreenCommitInterceptor</value>
			<value>eScreenNotificationInterceptor</value>
			<value>eScreenResponseInterceptor</value>
		</list>
		</property>
	</bean>
	
	<bean id="i3screenResultProcessContext" class="com.abc.service.integrations.vendor.fw.VendorResultProcessContext" >
  		<property name="contextName" value="I3"/>
  		<property name="requestProcessor" ref="i3screenRequestProcessor"/>
  	</bean>
  	
  	<bean id="i3screenRequestProcessor"  class="org.springframework.aop.framework.ProxyFactoryBean">
		<property name="target">
			<bean class="com.abc.service.integrations.vendor.fw.i3screen.I3screenRequestProcessor"/>
		</property>
		<property name="proxyTargetClass"><value>true</value></property>
		<property name="interceptorNames">
		<list>
			<!-- Interceptors -->
			<!-- Duplicate check, based on provider reference number (ex. 2012082210005) and vendor code (I3) -->
			<value>i3screenDuplicateResultCheckInterceptor</value>
			<!-- Try to find a subject match -->
			<value>i3screenFinderInterceptor</value>			
			<!-- Determine the i3Screen DT Result Status: Positive, Negative, Negative-Dilute, Non-Negative, etc. -->
			<value>i3screenResultStatusInterceptor</value>
			<!-- Determine if the DT results have pending (P) search match in the DB -->
			<value>i3screenRequestStatusInterceptor</value>	
			<!-- If special client, don't put negative dilute results in the basket. By default, negative dilute goes to the basket -->
			<value>i3screenNegativeDiluteInterceptor</value>		
			<!-- If negative results and match found, complete the Drug "D%" and MRON Components -->
			<value>i3screenNegativeResultInterceptor</value>
			<!-- Identify is we have non-negative results -->
			<value>i3screenNonNegativeResultInterceptor</value>
			<!-- Commit -->
			<value>i3screenCommitInterceptor</value>
			<!-- Send email -->
			<value>i3screenNotificationInterceptor</value>
			<!-- Prepare response -->
			<value>i3screenResponseInterceptor</value>
		</list>
		</property>
	</bean>
	
