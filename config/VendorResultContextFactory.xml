	<bean id="omegaRequestProcessor"  class="org.springframework.aop.framework.ProxyFactoryBean">
		<property name="target">
			<bean class="com.abc.service.integrations.vendor.fw.omega.OmegaRequestProcessor"/>
		</property>
		<property name="proxyTargetClass"><value>true</value></property>
		<property name="interceptorNames">
		<list>
			<!-- Interceptors -->
			<value>omegaDuplicateResultCheckInterceptor</value>
			<value>omegaFinderInterceptor</value>
			<value>omegaResultStatusInterceptor</value>
			<value>omegaRequestStatusInterceptor</value>
			<value>omegaNegativeResultInterceptor</value>
			<value>omegaNonNegativeResultInterceptor</value>
			<value>omegaCommitInterceptor</value>
			<value>omegaNotificationInterceptor</value>
			<value>omegaResponseInterceptor</value>
		</list>
		</property>
	</bean>
  	
  	<bean id="omegaResultProcessContext" class="com.abc.service.integrations.vendor.fw.VendorResultProcessContext" >
  		<property name="contextName" value="OMEGA"/>
  		<property name="requestProcessor" ref="omegaRequestProcessor"/>
  	</bean>
  	
  	
